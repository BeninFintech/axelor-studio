<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.0.xsd">

  <grid name="ws-request-grid" model="com.axelor.studio.db.WsRequest" title="Request">
    <field name="wsRequest"/>
    <field name="studioApp"/>
    <field name="sequence" hidden="true"/>
  </grid>

  <form name="ws-request-list-form" model="com.axelor.studio.db.WsRequestList" title="Request"
    onNew="action-ws-request-attrs-onnew" onLoad="action-ws-request-attrs-onnew">
    <panel name="mainPanel">
      <field name="wsRequest"/>
    </panel>
  </form>

  <action-attrs name="action-ws-request-attrs-onnew">
    <attribute name="hidden" for="studioApp" expr="eval: __parent__ != null"/>
  </action-attrs>

</object-views>
