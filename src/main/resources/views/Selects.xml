<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.0.xsd">

  <selection name="bpm.model.status.select">
    <option value="1">New</option>
    <option value="2">On Going</option>
    <option value="3">Terminated</option>
  </selection>

  <selection name="chart.wkf.bpm.manager.type.select">
    <option value="lateTask">Late tasks</option>
    <option value="nonLateTask">Not late tasks</option>
